<script type="text/javascript">

    $(document).ready(function () {

        $(document).on('click', '.point-edit', function () { 
            var jsondata = {
                id: $(this).parents('tr').attr('data-uid')
            };

            $.ajax({
                type: 'POST',
                contentType: "application/json; charset=utf-8",
                url: '/Point/GetPoint',
                async: false,
                data: JSON.stringify(jsondata),
                success: function (data) {
                    $('#hdnID').val(data.id);
                    $('#txtName').val(data.name);
                    $('#txtDescription').val(data.description);
                    $('#txtObjectType').val(data.objectType);
                    $('#txtObjectId').val(data.objectId);
                    $('#txtAssetId').val(data.assetId);
                    $('#chkArchive').prop('checked', data.archive);
                    //$('#txtLastUpdated').val(data.lastUpdated);
                    $('#txtAddedBy').val(data.addedBy);
                    $('#txtDeviceId').val(data.deviceId);
                    
                    ChangeAddEdit(1);
                }
            });
        });
        $(document).on('click', '.point-view', function () {
            alert($(this).parents('tr').attr('data-uid'));
        });
        $(document).on('click', '.point-duplicate', function () {
            var jsondata = {
                id: $(this).parents('tr').attr('data-uid')
            };

            $.ajax({
                type: 'POST',
                contentType: "application/json; charset=utf-8",
                url: '/Point/GetPoint',
                async: false,
                data: JSON.stringify(jsondata),
                success: function (data) {
                    $('#txtName').val(data.name);
                    $('#txtDescription').val(data.description);
                    $('#txtObjectType').val(data.objectType);
                    $('#txtObjectId').val(data.objectId);
                    $('#txtAssetId').val(data.assetId);
                    $('#chkArchive').prop('checked', data.archive);
                    //$('#txtLastUpdated').val(data.lastUpdated);
                    $('#txtAddedBy').val(data.addedBy);
                    $('#txtDeviceId').val(data.deviceId);
                }
            });
        });
        $(document).on('click', '.point-delete', function () {
            var jsondata = {
                id: $(this).parents('tr').attr('data-uid')
            };

            $.ajax({
                type: 'POST',
                contentType: "application/json; charset=utf-8",
                url: '/Point/DeletePoint',
                async: false,
                data: JSON.stringify(jsondata),
                success: function (data) {
                    var grid = $("#Grid").data("kendoGrid");

                    grid.dataSource.read();
                }
            });
        });

        $("#importUpload").on("dragenter", function (evt) {
            evt.preventDefault();
            evt.stopPropagation();
        });

        $("#importUpload").on("dragover", function (evt) {
            evt.preventDefault();
            evt.stopPropagation();
        });

        $("#importUpload").on("drop", function (evt) {
            evt.preventDefault();
            evt.stopPropagation();

            var files = evt.originalEvent.dataTransfer.files;

            if (files.length > 0) {
                $('#importFile')[0].files = files;
                AfterUpload(files[0].name);
            }

        });
        
     
    });
    function ChangeAddEdit(type) {
        if (type == 1) {
            $('#addPanel').hide();
            $('#editPanel').show();
        }
        else if (type == 2) {
            $('#addPanel').show();
            $('#editPanel').hide();
        }

    }
