                            columns.Bound(p => p.objectType).Title("Object Type");
                            columns.Bound(p => p.archive).Title("Archive");
                            columns.Bound(p => p.lastUpdated).Title("Last Updated");
                            columns.Bound(p => p.addedBy).Title("Added By");
                            columns.Command(command => { command.Custom("Details").Click("showDetails");
