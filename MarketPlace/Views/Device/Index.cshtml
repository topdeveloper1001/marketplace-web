<script type="text/javascript">

    $(document).ready(function () {

        $(document).on('click', '.device-edit', function () { 
            var jsondata = {
                id: $(this).parents('tr').attr('data-uid')
            };

            $.ajax({
                type: 'POST',
                contentType: "application/json; charset=utf-8",
                url: '/Device/GetDevice',
                async: false,
                data: JSON.stringify(jsondata),
                success: function (data) {
                    $('#hdnID').val(data.id);
                    $('#txtDeviceIdentifier').val(data.deviceIdentifier);
                    $('#txtIpAddress').val(data.ipAddress);
                    $('#txtType').val(data.type);
                    $('#txtClientId').val(data.clientId);
                    $('#txtBuildingId').val(data.buildingId);
                    $('#txtNetworkId').val(data.networkId);
                    
                    ChangeAddEdit(1);
                }
            });
        });
        $(document).on('click', '.device-view', function () {
            alert($(this).parents('tr').attr('data-uid'));
        });
        $(document).on('click', '.device-duplicate', function () {
            var jsondata = {
                id: $(this).parents('tr').attr('data-uid')
            };

            $.ajax({
                type: 'POST',
                contentType: "application/json; charset=utf-8",
                url: '/Device/GetDevice',
                async: false,
                data: JSON.stringify(jsondata),
                success: function (data) {
                    $('#txtDeviceIdentifier').val(data.deviceIdentifier);
                    $('#txtIpAddress').val(data.ipAddress);
                    $('#txtType').val(data.type);
                    $('#txtClientId').val(data.clientId);
                    $('#txtBuildingId').val(data.buildingId);
                    $('#txtNetworkId').val(data.networkId);
                }
            });
        });
        $(document).on('click', '.device-delete', function () {
            var jsondata = {
                id: $(this).parents('tr').attr('data-uid')
            };

            $.ajax({
                type: 'POST',
                contentType: "application/json; charset=utf-8",
                url: '/Device/DeleteDevice',
                async: false,
                data: JSON.stringify(jsondata),
                success: function (data) {
                    var grid = $("#Grid").data("kendoGrid");

                    grid.dataSource.read();
                }
            });
        });

        $("#importUpload").on("dragenter", function (evt) {
            evt.preventDefault();
            evt.stopPropagation();
        });

        $("#importUpload").on("dragover", function (evt) {
            evt.preventDefault();
            evt.stopPropagation();
        });

        $("#importUpload").on("drop", function (evt) {
            evt.preventDefault();
            evt.stopPropagation();

            var files = evt.originalEvent.dataTransfer.files;

            if (files.length > 0) {
                $('#importFile')[0].files = files;
                AfterUpload(files[0].name);
            }

        });
     
    });

    function ExportDevices() {
        //document.location.href = "https://localhost:44364/devices/devices_" + Date.now() + ".csv";
        document.location.href = "https://marketplaceapicore.azurewebsites.net/devices/devices_" + Date.now() + ".csv";

    }

    function ImportDevices() {
        
        if (!$('#importFile')[0].files || !$('#importFile')[0].files[0]) {
            alert("select file to import");
            return;
        }
        var formData = new FormData();
        formData.append("file", $('#importFile')[0].files[0]);
        $.ajax({
            type: "POST",
            //url: 'https://localhost:44364/devices/import',
            url: 'Device/ImportDevice',
            
            contentType: false,
            processData: false,
            data: formData, 	
            async: false,
            success: function (data) {
                var grid = $("#Grid").data("kendoGrid");

                grid.dataSource.read();
            },
            error: function (error) {
                alert("failed");
            }
        });
    }

    function ChangeAddEdit(type) {
        if (type == 1) {
            $('#addPanel').hide();
            $('#editPanel').show();
        }
        else if (type == 2) {
            $('#addPanel').show();
            $('#editPanel').hide();
        }

    }
    function AddDevice() {

        var clientData = {
            deviceIdentifier: $('#txtDeviceIdentifier').val(),
            ipAddress: $('#txtIpAddress').val(),
            type: $('#txtType').val(),
            clientId: $('#txtClientId').val(),
            buildingId: $('#txtBuildingId').val(),
            networkId: $('#txtNetworkId').val()
        };
        
        $.ajax({
            type: 'POST',
            contentType: "application/json; charset=utf-8",
            url: '/Device/AddDevice',
            data: JSON.stringify(clientData),
            async: false,
            success: function (data) {
                var grid = $("#Grid").data("kendoGrid");

                grid.dataSource.read();
            }
        });
    }
    function UpdateDevice() {

        var clientData = {
            id: $('#hdnID').val(),
            deviceIdentifier: $('#txtDeviceIdentifier').val(),
            ipAddress: $('#txtIpAddress').val(),
            type: $('#txtType').val(),
            clientId: $('#txtClientId').val(),
            buildingId: $('#txtBuildingId').val(),
            networkId: $('#txtNetworkId').val()
        };

        $.ajax({
            type: 'POST',
            contentType: "application/json; charset=utf-8",
            url: '/Device/UpdateDevice',
            data: JSON.stringify(clientData),
            async: false,
            success: function (data) {
                var grid = $("#Grid").data("kendoGrid");

                grid.dataSource.read();
            }
        });
        CancelEdit();
    }
    function ClearForm() {
        $('#txtDeviceIdentifier').val("");
        $('#txtIpAddress').val("");
        $('#txtType').val("");
        $('#txtClientId').val("");
        $('#txtBuildingId').val("");
        $('#txtNetworkId').val("");
    }
    function CancelEdit() {

        ClearForm();
        ChangeAddEdit(2);

    }
    
    function error_handler(e) {
        if (e.errors) {
            var message = "Errors:\n";
            $.each(e.errors, function (key, value) {
                if ('errors' in value) {
                    $.each(value.errors, function () {
                        message += this + "\n";
                    });
                }
            });
            alert(message);
        }
    }


